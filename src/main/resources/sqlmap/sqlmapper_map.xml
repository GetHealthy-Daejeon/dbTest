<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.dbTest.mapper.ExcelMapper">
		
	<select id="facilityList" resultType="map">
	
	</select>
	
	<select id="selectYuseong" resultType="map">
		SELECT * 
		from dw.health_addr
		 	
	</select>
	
	<select id="selectDaedeok" resultType="map">
		SELECT * 
		from dw.health_addr
	</select>
	
	<select id="selectDong" resultType="map">
		SELECT *
		from dw.health_addr
	</select>

	<select id="selectJung" resultType="map">
		SELECT * 
		from dw.health_addr
		WHERE gu_code = "중구"
	</select>
	
	<select id="selectSeo" resultType="map">
		SELECT * 
		from dw.health_addr
	</select>
	
	
	<!-- 종목 카테고리별 조회(보류) -->
	<select id="selectEventCode" resultType="map">
		SELECT * 
		from dw.health_addr
		WHERE event_code = #{eventCode}
	</select>
	
</mapper>